<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="149.0"
          prefWidth="347.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
          fx:controller="ba.unsa.etf.ugradbeni.server.controller.AddRoomController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="222.0" minWidth="10.0" prefWidth="88.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="365.0" minWidth="10.0" prefWidth="259.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="113.0" minHeight="10.0" prefHeight="87.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="74.0" minHeight="10.0" prefHeight="62.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <TextField fx:id="roomName" GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </GridPane.margin>
        </TextField>
        <Label prefHeight="21.0" prefWidth="227.0" text="Room name:">
            <GridPane.margin>
                <Insets bottom="2.0" left="5.0" right="2.0" top="2.0"/>
            </GridPane.margin>
        </Label>
        <HBox alignment="TOP_RIGHT" prefHeight="41.0" prefWidth="299.0" spacing="6.0" GridPane.columnIndex="1"
              GridPane.rowIndex="1">
            <children>
                <Button cancelButton="true" mnemonicParsing="false" onAction="#cancleAction" text="Cancel"/>
                <Button defaultButton="true" mnemonicParsing="false" onAction="#addRoomAction" text="Ok"/>
            </children>
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </GridPane.margin>
            <padding>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
            </padding>
        </HBox>
    </children>
</GridPane>
